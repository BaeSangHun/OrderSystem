<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="bedata">
	<insert id="add" parameterType="besave">
		insert into BEData(username,be_date,b_metabolism,e_requirement,age)
		values(#{username},CURDATE(),#{Bmetabolism},#{Erequirement},#{age})
	</insert>
</mapper>