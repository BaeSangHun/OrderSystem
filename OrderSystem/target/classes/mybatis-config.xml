<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>

	<typeAliases>
		<typeAlias type="kr.ac.kopo.model.QuestionComment" alias="QComment"/>
		<typeAlias type="kr.ac.kopo.model.ReviewBoardComment" alias="RComment"/>
		<typeAlias type="kr.ac.kopo.model.NoticeComment" alias="NComment"/>
		<typeAlias type="kr.ac.kopo.util.FileVO" alias="FileVO"/>
		<typeAlias type="kr.ac.kopo.util.SearchVO" alias="SearchVO"/>
		<typeAlias type="kr.ac.kopo.util.PageVO" alias="PageVo"/>
		<typeAlias type="kr.ac.kopo.model.Routine" alias="Routine"/>
		<typeAlias type="kr.ac.kopo.model.Question" alias="Question"/>
		<typeAlias type="kr.ac.kopo.model.ReviewBoard" alias="ReviewBoard"/>
		<typeAlias type="kr.ac.kopo.model.Bucket" alias="Bucket"/>
		<typeAlias type="kr.ac.kopo.model.UserVO" alias="UserVO"/>
		<typeAlias type="kr.ac.kopo.model.TrainerProfile" alias="TrainerProfile"/>
		<typeAlias type="kr.ac.kopo.model.ExerciseJournal" alias="ExerciseJournal"/>
		<typeAlias type="kr.ac.kopo.model.ExerciseContents" alias="ExerciseContents"/>
		<typeAlias type="kr.ac.kopo.model.basicInformation" alias="basicInformation"/>
		<typeAlias type="kr.ac.kopo.model.BEsave" alias="besave"/>
		<typeAlias type="kr.ac.kopo.model.TypeOfExercise" alias="TypeOfExercise"/>
		<typeAlias type="kr.ac.kopo.model.UserManagement" alias="UserManagement"/>
		<typeAlias type="kr.ac.kopo.model.MentiPerformance" alias="MentiPerformance"/>
		<typeAlias type="kr.ac.kopo.model.MessageRepository" alias="MessageRepository"/>
	</typeAliases>

 	<mappers>
 		<mapper resource="mybatis/routine.xml" />
		<mapper resource="mybatis/question.xml"/>
		<mapper resource="mybatis/reviewboard.xml"/>
		<mapper resource="mybatis/user.xml"/>
		<mapper resource="mybatis/trainer.xml"/>
		<mapper resource="mybatis/statistics.xml"/>
		<mapper resource="mybatis/bedata.xml"/>
		<mapper resource="mybatis/message.xml"/>
	</mappers>
</configuration>